#!/bin/bash

find . \(					\
     -name '*.fas' -o				\
     -name '*.fasl' -o				\
     -name '*.xfasl' -o				\
     -name '*.x86f' -o				\
     -name '*.o' -o				\
     -name '*.lo' -o				\
     -name '*.a' -o				\
     -name '*.la' -o				\
     -name '#*' -o				\
     -name '.DS_Store' -o			\
     -name '.localized'				\
     \) -type f -print \
| \
while read file
do
    /bin/rm -f "$file"
done
