  list.bahai.srp                                \
  list.bahai.study                              \
  mail.archive                                  \
  mail.spam                                     \
  mail.trash                                    \
  mail.sent                                     \
  list.github                                   \
  list.notifications                            \
  list.misc                                     \
  mail.gmail                                    \
  mail.gmail.kadena                             \
  mail.gmail.c2g

for i in $(msgcounts | field 1 | tail -n +2)
do
    doveadm expunge mailbox $i DELETED
done
