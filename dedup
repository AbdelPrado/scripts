#!/bin/bash

IMAP=$HOME/.nix-profile/libexec/dovecot/imap

python3 ~/src/IMAPdedup/imapdedup.py            \
  -P "$IMAP -c /etc/dovecot/dovecot.conf"       \
  -u johnw -w pass -S -c -m                     \
  mail.kadena                                   \
  mail.kadena.archive                           \
  INBOX                                         \
  Drafts                                        \
  list.kadena                                   \
