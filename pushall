#!/bin/bash

pushme -F "$@"

if [[ -f /Volumes/tidbit/Data/Documents/Tasks/todo.txt ]]; then
    pushme tidbit "$@"
fi

if [[ $(hostname) == "Vulcan.local" || $(hostname) == "vulcan.local" ]]; then
    pushme titan "$@"
    #pushme -Q storage "$@"
fi

sudo cleanup -u; sudo cleanup -u; sudo cleanup -u

### pushall ends here
