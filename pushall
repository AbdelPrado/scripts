#!/bin/bash

pushme -F "$@"

if [[ -d /Volumes/Vault/Dropbox ]]; then
    pushme vault "$@"
fi

if [[ -d "/Volumes/Macintosh HD 1" ]]; then
    pushme clone "$@"
fi

#if [[ $(hostname) == "Vulcan.local" || $(hostname) == "vulcan.local" ]]; then
pushme titan "$@"
#fi

(sudo cleanup -u; sudo cleanup -u; sudo cleanup -u) > /dev/null 2>&1

### pushall ends here
