#!/bin/bash

open ~/Applications/Firefox.app

open /Applications/Background\ Music.app
open /Applications/CCMenu.app
open /Applications/Fantastical\ 2.app
open /Applications/Keybase.app
open /Applications/Keyboard\ Maestro.app
open /Applications/Messages.app
open /Applications/Monosnap.app
open /Applications/Path\ Finder.app
open /Applications/Slack.app
open /Applications/VPNStatus.app
open /Applications/Vagrant\ Manager.app
open /Applications/iTerm.app
open /Applications/zoom.us.app

if [[ $(hostname) =~ [Vv]ulcan.* ]]; then
   open /Applications/Dragon\ Dictate.app
   open /Applications/teleport.app
   open /Applications/ScanSnap\ Manager.app
fi
