#!/usr/bin/env bash -e

cd ~/org
changes

for dir in                                      \
    ~/doc                                       \
    ~/doc/accounts                              \
    ~/src/scripts                               \
    ~/src/nix                                   \
    ~/src/org-data                              \
    ~/.config/pushme/conf.d
do
    cd $dir
    changes
done

cd ~/.emacs.d
load-env-emacs29MacPort make
changes
