#!/bin/bash
exec rsync		       \
    --exclude=.Caches/	       \
    --exclude=.DS_Store	       \
    --exclude=.Spotlight-V100/ \
    --exclude=.TemporaryItems/ \
    --exclude=.Trashes/	       \
    --exclude=.fseventsd/      \
    --exclude=.localized       \
    --exclude=Products/	       \
    -a --delete "$@"
